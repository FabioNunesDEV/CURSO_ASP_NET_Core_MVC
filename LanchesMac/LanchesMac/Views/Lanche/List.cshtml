@model IEnumerable<LanchesMac.Models.Lanche>

<hr />
<h2>@ViewBag.titulo</h2>
<h4>@ViewBag.totalLabel @ViewBag.total</h4>
<p>Data atual : @ViewBag.data.ToShortDateString()</p>
<hr />


@foreach (var lanche in Model)
{
    <div>
        <h4>Lanche : @lanche.Nome</h4>
        <p><img src="@lanche.ImagemThumbnailUrl"/></p>
        <p>Preço : @lanche.Preco.ToString("c")</p>
    </div>
}
